{
  "name": "@local/ui-thumbnail",
  "version": "0.0.0",
  "private": true,
  "description": "Shared Thumbnail component (React + Taro entrypoints) for the tea project",
  "main": "dist/react/index.js",
  "types": "dist/react/index.d.ts",
  "files": ["dist"],
  "license": "UNLICENSED",
  "scripts": {
    "build:react": "tsc -p react/tsconfig.json",
    "build:taro": "tsc -p taro/tsconfig.json",
    "build": "npm run build:react && npm run build:taro && node ./scripts/copy-assets.js"
  },
  "devDependencies": {
    "typescript": "^5.5.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/node": "^18.0.0",
    "@tarojs/components": "^4.1.8",
    "rollup": "^4.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2"
  },
  "exports": {
    "./react": "./dist/react/index.js",
    "./taro": "./dist/taro/index.js",
    "./package.json": "./package.json",
    "./*": "./dist/*",
    ".": "./dist/react/index.js"
  }
  ,
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
