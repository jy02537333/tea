#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/app/wx-fe/node_modules/.pnpm/vite@4.5.14_@types+node@22.19.3_less@4.5.1_lightningcss@1.30.2_sass@1.97.1_stylus@0.64.0_terser@5.44.1/node_modules/vite/bin/node_modules:/app/wx-fe/node_modules/.pnpm/vite@4.5.14_@types+node@22.19.3_less@4.5.1_lightningcss@1.30.2_sass@1.97.1_stylus@0.64.0_terser@5.44.1/node_modules/vite/node_modules:/app/wx-fe/node_modules/.pnpm/vite@4.5.14_@types+node@22.19.3_less@4.5.1_lightningcss@1.30.2_sass@1.97.1_stylus@0.64.0_terser@5.44.1/node_modules:/app/wx-fe/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/app/wx-fe/node_modules/.pnpm/vite@4.5.14_@types+node@22.19.3_less@4.5.1_lightningcss@1.30.2_sass@1.97.1_stylus@0.64.0_terser@5.44.1/node_modules/vite/bin/node_modules:/app/wx-fe/node_modules/.pnpm/vite@4.5.14_@types+node@22.19.3_less@4.5.1_lightningcss@1.30.2_sass@1.97.1_stylus@0.64.0_terser@5.44.1/node_modules/vite/node_modules:/app/wx-fe/node_modules/.pnpm/vite@4.5.14_@types+node@22.19.3_less@4.5.1_lightningcss@1.30.2_sass@1.97.1_stylus@0.64.0_terser@5.44.1/node_modules:/app/wx-fe/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
